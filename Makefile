sll:
	g++ -std=c++20 singly_linked_list.cpp && ./a.out && rm ./a.out
dll:
	g++ -std=c++20 doubly_linked_list.cpp && ./a.out && rm ./a.out
it:
	g++ -std=c++20 iterator.cpp && ./a.out && rm ./a.out
rev:
	g++ -std=c++20 pointers.cpp && ./a.out && rm ./a.out
bst:
	g++ -std=c++20 bst.cpp && ./a.out && rm ./a.out
